                {
                  "name": "formatResults",
                  "isOptional": true
                },
                {
                  "name": "query",
                  "value": "advisorresources\n| where isnotnull(properties.impactedField)\n| extend Category = properties.category\n| extend Impact = properties.impact\n| extend ResourceType = properties.impactedField\n| extend ResourceName = properties.impactedValue\n| extend Problem = properties.shortDescription.problem\n| extend Solution = properties.shortDescription.solution\n| extend ImpactId = case(tostring(properties.impact) =~ \"High\", 1, tostring(properties.impact) =~ \"Medium\", 2, 3)\n| order by ImpactId asc, tostring(properties.category) asc\n| project Impact, Category, ResourceType, ResourceName, Problem, id",
                  "isOptional": true
                }
              ],
              "type": "Extension/HubsExtension/PartType/ArgQueryGridTile",
              "settings": {}
            }
