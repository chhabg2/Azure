                {
                  "name": "formatResults",
                  "value": true,
                  "isOptional": true
                },
                {
                  "name": "query",
                  "value": "Resources\n| where type =~ 'Microsoft.Network/virtualNetworks'\n| extend peering=properties.virtualNetworkPeerings\n| where array_length(peering) > 0\n| mvexpand peering\n| extend AllowVirtualNetworkAccess = peering.properties.allowVirtualNetworkAccess\n| extend AllowForwardedTraffic = peering.properties.allowForwardedTraffic\n| extend AllowGatewayTransit = peering.properties.allowGatewayTransit\n| extend UseRemoteGateways = peering.properties.useRemoteGateways\n| extend PeeringState = peering.properties.peeringState\n| extend RemoteVirtualNetworkId = peering.properties.remoteVirtualNetwork.id\n| project subscriptionId, resourceGroup, id, AllowVirtualNetworkAccess, AllowForwardedTraffic, AllowGatewayTransit, UseRemoteGateways, RemoteVirtualNetworkId",
                  "isOptional": true
                }
              ],
              "type": "Extension/HubsExtension/PartType/ArgQueryGridTile",
              "settings": {}
            }
