                  "isOptional": true
                },
                {
                  "name": "query",
                  "value": "resources\n| where type =~ 'microsoft.network/publicipaddresses' or type =~ \"microsoft.classicnetwork/reservedips\"\n| extend DeploymentType = iif(type == \"microsoft.classicnetwork/reservedips\", \"Classic\", \"ARM\")\n| extend AttachedTo = iff(type == \"microsoft.classicnetwork/reservedips\", \n\tsplit(properties.attachedTo.id, '/')[8], \n\tsplit(properties.ipConfiguration.id, '/')[8]\n)\n| project subscriptionId, resourceGroup, id, DeploymentType, ['IP Address']=properties.ipAddress, SKU=sku.name, ['Allocation Method']=properties.publicIPAllocationMethod, AttachedTo, properties\n",
                  "isOptional": true
                }
              ],
              "type": "Extension/HubsExtension/PartType/ArgQueryGridTile",
              "settings": {}
            }
